{
  "name": "playground",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "tsc": "tsc",
    "build": "rm -rf dist && tsc && rollup -c rollup.config.js",
    "build:watch": "tsc -w --incremental --preserveWatchOutput",
    "start:ui": "concurrently --names tsc,dev-server \"npm run build:watch\" \"web-dev-server --config ./web-dev-server.config.mjs\"",
    "package": "rm -rf ui.zip && cd dist && bestzip ../ui.zip * && cd .. && hc web-app pack ./webhapp.workdir",
    "clean:hc": "rm -rf .hc*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "21.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-typescript": "^10.0.0",
    "@web/dev-server": "^0.1.35",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/rollup-plugin-html": "^1.11.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "bestzip": "^2.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "new-port-cli": "^1.0.0",
    "run-singleton-cli": "^0.0.7",
    "deepmerge": "^4.2.2",
    "exits": "^2.0.1",
    "path-exists-cli": "^2.0.0",
    "rollup": "^2.56.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@ddd-qc/cell-proxy": "0.10.1",
    "@ddd-qc/lit-happ": "0.10.1",
    "@ddd-qc/agent-directory": "file:../../webcomponents",
    "@open-wc/scoped-elements": "^2.1.1",
    "@holochain/client": "^0.11.9",
    "@holochain-open-dev/core-types": "^0.6.3",
    "@lit-labs/context": "^0.1.2",
    "lit": "^2.3.0"
  }
}
